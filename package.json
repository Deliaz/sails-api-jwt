{
  "name": "sails-api-jwt",
  "private": false,
  "version": "1.2.1",
  "description": "An example implementation of JWT-based API for user registration and authorization.",
  "keywords": [
    "Sails",
    "Sails.js",
    "JWT",
    "JSON Web Token",
    "authorization",
    "API"
  ],
  "dependencies": {
    "bcrypt": "^1.0.3",
    "farmhash": "^2.0.3",
    "include-all": "^4.0.3",
    "jsonwebtoken": "^8.0.1",
    "mailgun-js": "^0.13.1",
    "moment": "^2.18.1",
    "password-validator": "^4.0.0",
    "rc": "1.2.2",
    "sails": "~0.12.14",
    "sails-disk": "~0.10.10",
    "shortid": "^2.2.8",
    "validator": "^9.1.1"
  },
  "scripts": {
    "debug": "node debug app.js",
    "start": "node app.js",
    "test": "nyc mocha --ui bdd test/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deliaz/sails-api-jwt.git"
  },
  "author": "deliaz",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.7.2",
    "mocha": "^4.0.1",
    "mock-require": "^2.0.2",
    "nyc": "^11.2.1",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.13.0"
  },
  "nyc": {
    "exclude": [
      "config",
